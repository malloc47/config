"xbacklight -inc 10"
    XF86MonBrightnessUp

"xbacklight -dec 10"
    XF86MonBrightnessDown

"amixer set Master 5%+"
    XF86AudioRaiseVolume

"amixer set Master 5%-"
    XF86AudioLowerVolume

"amixer set Master toggle"
    XF86AudioMute

"kbdlight up 25"
    XF86KbdBrightnessUp

"kbdlight down 25"
    XF86KbdBrightnessDown

"emacsclient -c"
    XF86LaunchA

"chromium"
    XF86LaunchB
